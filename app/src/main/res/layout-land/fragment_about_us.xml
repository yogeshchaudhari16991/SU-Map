<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools" android:layout_width="match_parent"
    android:layout_height="match_parent" tools:context="com.example.yogesh16991.test_proj.AboutUs">

    <!-- TODO: Update blank fragment layout -->

    <RelativeLayout
        android:layout_width="fill_parent"
        android:layout_height="fill_parent"
        android:layout_gravity="center_horizontal|bottom"
        android:background="@drawable/aboutus">

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textAppearance="?android:attr/textAppearanceMedium"
            android:text="Yogesh Chaudhari"
            android:id="@+id/textView7"
            android:textStyle="bold|italic"
            android:textColor="#ff000000"
            android:layout_marginLeft="15dp"
            android:layout_above="@+id/textView8"
            android:layout_alignStart="@+id/textView6" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textAppearance="?android:attr/textAppearanceMedium"
            android:text="Sayali Khodke"
            android:id="@+id/textView8"
            android:textStyle="bold|italic"
            android:textColor="#ff000000"
            android:layout_marginBottom="30dp"
            android:layout_alignParentBottom="true"
            android:layout_toEndOf="@+id/textView7" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textAppearance="?android:attr/textAppearanceMedium"
            android:text="Rajas Save"
            android:id="@+id/textView9"
            android:textStyle="bold|italic"
            android:textColor="#ff000000"
            android:layout_alignParentBottom="true"
            android:layout_toEndOf="@+id/textView8"
            android:layout_marginBottom="10dp" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textAppearance="?android:attr/textAppearanceLarge"
            android:text="Android Programmming"
            android:id="@+id/textView6"
            android:layout_marginTop="50dp"
            android:layout_alignParentTop="true"
            android:layout_centerHorizontal="true"
            android:textColor="#ff050505"
            android:textStyle="bold"
            android:textSize="33dp"
            android:textIsSelectable="true"
            android:textColorHighlight="#ff083cff" />
    </RelativeLayout>

</FrameLayout>
